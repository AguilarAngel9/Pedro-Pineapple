    "            action == Actions.down_movement.value and relative_change <= -0.1\n",
    "        ):\n",
    "            step_reward = price_diff\n",
    "        elif (\n",
    "            action == Actions.no_movement and abs(relative_change) < 0.1\n",
    "        ):\n",
    "            step_reward = price_diff\n",
    "\n",
    "        return abs(step_reward)\n",
    "\n",
    "    def _update_profit(self, action):\n",
